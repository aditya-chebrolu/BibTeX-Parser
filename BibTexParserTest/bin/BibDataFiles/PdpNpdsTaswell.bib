}

@InProceedings{Taswell2015AMA,
  author    = {S. Koby Taswell and Adam Craig and Diana Leung and Stephan Loh and Martin Skarzynski and Sara Gharabaghi and Bohan Zhou and Carl Taswell},
  booktitle = {Proceedings Annual Meeting of the Western Region Society of Nuclear Medicine},
  title     = {Hypothesis-Exploring Methods for Automated Meta-Analyses of Brain Imaging Literature},
