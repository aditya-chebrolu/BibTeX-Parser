@inproceedings{Brank2005,
  author = {Janez Brank and Marko Grobelnik and Dunja Mladenic},
  booktitle = {SIKDD 2005},
  title = {A Survey of Ontology Evaluation Techniques},
  abstract = {An ontology is an explicit formal conceptualization of some domain
	of interest. Ontologies are increasingly used in various fields such
	as knowledge management, information extraction, and the semantic
	web. Ontology evaluation is the problem of assessing a given ontology
	from the point of view of a particular criterion of application,
