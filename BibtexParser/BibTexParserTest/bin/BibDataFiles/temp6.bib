  keywords = {Search engines; content management; Web page; Organizing; Unsupervised learning; fuzzy set theory; open systems; Web pages; XML-based technologies; content retrieval; semantic Web content analysis; pattern clustering; metadata; Web sites; Web search engines; Information retrieval; collaborative proximity-based fuzzy clustering; semantic vision; Web search; comprehensive data organization; Web information; content filtering; interoperability; Semantic Web; automated tools; groupware; spaces of semantics and data; proximity-based collaborative clustering; Information filters; Collaboration; Information filtering; Content based retrieval; semantic Web document; Internet; World Wide Web},
