  keywords = {Animals; Bibliography; Databases, Genetic; Electronic Mail; Genes; Genomics; Humans; Information Storage and Retrieval; Internet; Molecular Biology; Proteins; Research Support, Non-U.S. Gov't; Research Support, U.S. Gov't, P.H.S.; Software; Terminology},
